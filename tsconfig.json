{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./src/assets/*"]
    },
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "strictNullChecks": true,
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"]
  }
}
